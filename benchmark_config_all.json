{
    "experiment_name": "benchmark_large_all",
    "datasource": [
        {
            "name": "large_data",
            "loader": "standard_loader",
            "data_path": "ml_framework/data/cleaned/6m_cleaned.csv",
            "val_split": 0.2,
            "balance_val": true,
            "target_column": "cvv",
            "drop_columns": [
                "event_id"
            ],
            "discard_classes": [
                1.0
            ],
            "balance_train": "oversample"
        }
    ],
    "normalization": [
        {
            "name": "std",
            "scaling": "standard"
        },
        {
            "name": "minmax",
            "scaling": "minmax"
        },
        {
            "name": "maxabs",
            "scaling": "maxabs"
        },
        {
            "name": "robust",
            "scaling": "robust"
        },
        {
            "name": "yeo_johnson",
            "scaling": "yeo-johnson"
        },
        {
            "name": "quantile_norm",
            "scaling": "quantile_normal"
        },
        {
            "name": "quantile_uni",
            "scaling": "quantile_uniform"
        }
    ],
    "method_n_jobs": 15,
    "methods": [
        "dummy_classifier",
        "logistic_regression",
        "random_forest",
        "gradient_boosting",
        "hist_gradient_boosting",
        "sgd_classifier",
        "mlp_classifier",
        "lda",
        "gaussian_nb",
        "polynomial_lr",
        "nystroem_sgd",
        "bagging_classifier",
        "voting_classifier",
        "kmeans_5",
        "nca_knn"
    ],
    "output": {
        "dir": "results/default_benchmark_all"
    }
}